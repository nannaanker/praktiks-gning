---
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import "../styles/tailwind.css";
import "animate.css";
const { title } = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/Subtract.webp" />
    <link rel="stylesheet" href="https://use.typekit.net/oep1tpx.css" />
    <link rel="stylesheet" href="https://use.typekit.net/oep1tpx.css" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>{Astro.props.title}</title>
  </head>

  <body class="bg-lyserod bg-[url('/baggrund2.webp')] bg-cover bg-fixed">
    <Header />

    <main>
      <slot />
    </main>
    <Footer />
  </body>
</html>
